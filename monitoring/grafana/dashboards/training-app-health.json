{
  "id": null,
  "uid": "training-app-health",
  "title": "Training App - Health",
  "tags": ["training-app", "observability"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "datasource",
        "label": "Prometheus",
        "query": "prometheus",
        "refresh": 1,
        "hide": 0,
        "current": {
          "text": "Prometheus",
          "value": "Prometheus"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Requests per second",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_request_duration_seconds_count[$__rate_interval]))",
          "legendFormat": "RPS"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Error rate (5xx)",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum(rate(http_request_duration_seconds_count{status_code=~\"5..\"}[$__rate_interval])) / sum(rate(http_request_duration_seconds_count[$__rate_interval]))) * 100",
          "legendFormat": "5xx %"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent" },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Latency P95",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "P95"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Process memory (RSS)",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "training_app_process_resident_memory_bytes",
          "legendFormat": "RSS"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes" },
        "overrides": []
      }
    }
  ]
}
